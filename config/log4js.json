{
  "appenders": {
    "out": {
      "type": "stdout",
      "layout": {
        "type": "pattern",
        "pattern": "%d %p %c %f:%l %m%n"
      }
    }
  },
  "categories": {
    "default": {
      "appenders": [
        "out"
      ],
      "level": "info",
      "enableCallStack": true
    }
  }
}
